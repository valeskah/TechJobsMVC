<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@foreach (Dictionary<string, string> jobItem in ViewBag.jobs)
{
    <table class="job-listing">
        @foreach (KeyValuePair<string, string> job in jobItem)
        {
            <tr>
                <td>@job.Key</td>
                <td>@job.Value</td>
            </tr>
        }
    </table>
    // TODO #3 - Use the same list display from Views/Search/Index.cshtml 
}